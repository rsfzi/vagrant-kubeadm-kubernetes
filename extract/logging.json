{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "console": {
      "format": "%(message)s"
    },
    "file": {
      "format": "%(asctime)s %(filename)16s(%(lineno)d) %(name)12s %(levelname)-7s: %(message)s"
    }
  },

  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "file",
      "stream": "ext://sys.stderr"
    },

    "file": {
      "class": "logging.FileHandler",
      "mode": "w",
      "level": "DEBUG",
      "formatter": "file",
      "filename": "/dev/null"
    }
  },

  "loggers": {
    "": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    },
    "pika": {
      "level": "INFO",
      "handlers": [
        "file"
      ],
      "propagate": false
    }
  }
}
